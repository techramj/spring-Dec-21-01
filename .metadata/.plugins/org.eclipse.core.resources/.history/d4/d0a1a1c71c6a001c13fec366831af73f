<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean name="bike" class="com.easylearning.Bike">
		<property name="name" value="Pulsar"></property>
	</bean>

	<bean name="car" class="com.easylearning.Car">
		<property name="name" value="BMW"></property>
	</bean>

	<bean name="emp" class="com.easylearning.Employee">
		<property name="id" value="1"></property>
		<property name="name" value="Jack"></property>
		<property name="salary" value="1000"></property>
		<property name="vehicle" ref="bike"></property>
	</bean>
   

</beans>
